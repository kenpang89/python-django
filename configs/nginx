server {
    listen 80;
    server_name 127.0.0.1;
    location = /favicon.ico {access_log off;log_not_found off;}

    location /static {
        alias /var/work/helloworld/static;
    }
    location /media {
        alias /var/work/helloworld/media;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/var/work/helloworld/helloworld.sock;
    }
}
